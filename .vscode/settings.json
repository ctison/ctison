{
  "[js][jsx][ts][tsx][vue][css][less][scss][sass][graphql][gql][md][mdx][yaml][json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.ignorePaths": [
    "package.json",
    "package-lock.json",
    "node_modules/",
    ".git/objects",
    ".vscode/settings.json",
    "pnpm-lock.yaml"
  ],
  "cSpell.words": [
    "arbitrum",
    "Devdotto",
    "fontsource",
    "gsap",
    "lifi",
    "linea",
    "moralis",
    "rainbowkit",
    "splinetool",
    "Tison",
    "unocss",
    "wagmi",
    "windi",
    "windicss",
    "zora"
  ],
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "pnpm-lock.yaml": true
  },
  "typescript.tsdk": "./node_modules/typescript/lib"
}
