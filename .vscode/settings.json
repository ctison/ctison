{
  "[js][jsx][ts][tsx][vue][css][less][scss][sass][graphql][gql][md][mdx][yaml][json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[solidity]": {
    "editor.defaultFormatter": "JuanBlanco.solidity"
  },
  "cSpell.ignorePaths": [
    "package.json",
    "package-lock.json",
    "node_modules/",
    ".git/objects",
    ".vscode/settings.json",
    "pnpm-lock.yaml"
  ],
  "cSpell.words": [
    "airtable",
    "arbitrum",
    "contentlayer",
    "devcontainer",
    "Devdotto",
    "echarts",
    "fontsource",
    "gsap",
    "kbox",
    "linea",
    "moralis",
    "nprogress",
    "rainbowkit",
    "reactflow",
    "rehype",
    "splinetool",
    "tanstack",
    "thirdweb",
    "Tison",
    "unocss",
    "uups",
    "wagmi",
    "windi",
    "windicss",
    "zora"
  ],
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "pnpm-lock.yaml": true
  },
  "solidity.formatter": "forge",
  "typescript.tsdk": "node_modules/typescript/lib",
  "solidity.compileUsingRemoteVersion": "0.8.23"
}
