{
  "name": "ctison",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier -w . ; sort-package-json",
    "format:check": "prettier --check . && sort-package-json --check",
    "lint": "next lint",
    "prepare": "husky install",
    "start": "next start"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "singleQuote": true
  },
  "dependencies": {
    "next": "13.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.6.5",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.2",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "sort-package-json": "2.5.1",
    "typescript": "5.2.2"
  }
}
